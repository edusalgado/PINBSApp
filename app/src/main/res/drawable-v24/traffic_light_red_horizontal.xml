<?xml version="1.0" encoding="utf-8"?><!--<shape xmlns:android="http://schemas.android.com/apk/res/android"--><!--android:shape="oval">--><!--<solid android:color="#199fff" />--><!--<stroke--><!--android:width="2dp"--><!--android:color="#444444" />--><!--<solid android:color="#199fff" />--><!--<stroke--><!--android:width="2dp"--><!--android:color="#c41616" />--><!--</shape>-->
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@+id/trafficLight_high"
        android:bottom="0dp"
        android:left="0dp"
        android:right="80dp"
        android:top="0dp">
        <shape android:shape="oval">
            <solid android:color="#d50000" />
            <stroke
                android:width="2dp"
                android:color="#d50000" />
            <size
                android:width="40dp"
                android:height="40dp" />
        </shape>
    </item>
    <item
        android:id="@+id/trafficLight_mid"
        android:bottom="0dp"
        android:left="40dp"
        android:right="40dp"
        android:top="0dp">
        <shape android:shape="oval">
            <solid android:color="#00ffffff" />
            <stroke
                android:width="2dp"
                android:color="#ffe100" />
            <size
                android:width="40dp"
                android:height="40dp" />
        </shape>
    </item>
    <item
        android:id="@+id/trafficLight_low"
        android:left="80dp"
        android:right="0dp"
        android:top="0dp">
        <shape android:shape="oval">
            <solid android:color="#00ffffff" />
            <stroke
                android:width="2dp"
                android:color="#64dd17" />
            <size
                android:width="40dp"
                android:height="40dp" />
        </shape>
    </item>
</layer-list>